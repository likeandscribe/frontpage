    "@markdoc/markdoc": "^0.4.0",
    "@next/env": "16.0.0-beta.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@repo/frontpage-atproto-client": "workspace:*",
    "@repo/frontpage-oauth": "workspace:*",
    "@vercel/analytics": "^1.5.0",
    "@vercel/toolbar": "^0.1.41",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.44.4",
    "jose": "^5.10.0",
    "next": "catalog:",
    "next-themes": "^0.4.6",
    "oauth4webapi": "^2.17.0",
    "radix-ui": "^1.4.2",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-error-boundary": "^4.1.2",
    "react-intersection-observer": "^9.16.0",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "swr": "^2.3.4",
    "tailwind-merge": "^2.6.0",
