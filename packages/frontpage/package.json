{
  "name": "frontpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=--use-openssl-ca next dev --hostname=0.0.0.0 --turbopack",
    "dev-1pw": "NODE_OPTIONS=--use-openssl-ca op run --env-file=\"./.env.1pw\" -- next dev --turbopack",
    "build-1pw": "NODE_OPTIONS=--use-openssl-ca op run --env-file=\"./.env.1pw\" -- next build",
    "build": "NODE_OPTIONS=--use-openssl-ca next build --turbopack",
    "start": "NODE_OPTIONS=--use-openssl-ca next start",
    "lint": "eslint .",
    "db:generate": "NODE_OPTIONS=--use-openssl-ca drizzle-kit generate",
    "db:migrate": "NODE_OPTIONS=--use-openssl-ca drizzle-kit migrate",
    "db:studio": "NODE_OPTIONS=--use-openssl-ca drizzle-kit studio",
    "db:push": "NODE_OPTIONS=--use-openssl-ca drizzle-kit push",
    "db:pull": "NODE_OPTIONS=--use-openssl-ca drizzle-kit pull",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "generate-local-env": "node scripts/generate-local-env.mts"
  },
  "dependencies": {
    "@atproto/common-web": "^0.3.2",
    "@atproto/oauth-types": "^0.1.5",
